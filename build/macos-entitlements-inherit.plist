<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!-- ============================================ -->
<!-- 🍎 macOS INHERITED ENTITLEMENTS             -->
<!-- ============================================ -->
<!-- Entitlements inherited by child processes    -->
<!-- (e.g., GPU process, renderer process)        -->
<!-- Platform: macOS 10.13+                       -->
<!-- ============================================ -->
<plist version="1.0">
<dict>
    <!-- ============================================ -->
    <!-- Code Execution (Inherited)                  -->
    <!-- ============================================ -->
    
    <!-- Enable JIT compilation for child processes -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Allow unsigned executable memory in child processes -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Disable library validation for child processes -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- ============================================ -->
    <!-- App Sandbox (Disabled)                      -->
    <!-- ============================================ -->
    
    <!-- Disable App Sandbox for child processes -->
    <key>com.apple.security.app-sandbox</key>
    <false/>
</dict>
</plist>

